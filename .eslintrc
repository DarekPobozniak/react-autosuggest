{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": "true",
    "mocha": "true"
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "settings": {
    "import/resolver": "webpack",
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  }
}

